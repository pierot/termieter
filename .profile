################################################################################################################
# ALIASES
alias clientrepos="cd ~/Documents/Dropbox/Private/work/Client\ Repos/"

################################################################################################################
# PATH
export PATH="/usr/local/sbin:/usr/local/mysql/bin:$PATH"
export PATH=$PATH:/usr/local/bin

# MacPorts Installer addition on 2010-03-06_at_17:05:01: adding an appropriate PATH variable for use with MacPorts.
export PATH=/opt/local/bin:/opt/local/sbin:$PATH
# MacPorts Installer addition on 2010-03-06_at_17:05:01: adding an appropriate MANPATH variable for use with MacPorts.
export MANPATH=/opt/local/share/man:$MANPATH

# Setting PATH for MacPython 2.6
# The orginal version is saved in .profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/2.6/bin:${PATH}"

export PATH
