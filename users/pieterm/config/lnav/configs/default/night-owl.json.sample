{
    "$schema": "https://lnav.org/schemas/config-v1.schema.json",
    "ui": {
        "theme-defs": {
            "night-owl": {
                "vars": {
                    "black": "#011627",
                    "red": "#ff6868",
                    "green": "#007f00",
                    "yellow": "#cdcd00",
                    "blue": "#5394ec",
                    "magenta": "#ff70ff",
                    "cyan": "#33cccc",
                    "white": "#d6deeb",
                    "semantic_highlight_color": "semantic()"
                },
                "styles": {
                    "identifier": {
                        "color": "semantic()"
                    },
                    "text": {
                        "color": "#d6deeb",
                        "background-color": "#011627"
                    },
                    "selected-text": {
                        "background-color": "#4373c2"
                    },
                    "fuzzy-match": {
                        "color": "$yellow",
                        "underline": true,
                        "bold": true
                    },
                    "alt-text": {
                        "background-color": "#15293a"
                    },
                    "ok": {
                        "color": "$green",
                        "bold": true
                    },
                    "info": {
                        "color": "$magenta",
                        "bold": true
                    },
                    "error": {
                        "color": "#ef5350",
                        "bold": true
                    },
                    "warning": {
                        "color": "#b39554",
                        "bold": true
                    },
                    "hidden": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "cursor-line": {
                        "color": "$cyan",
                        "background-color": "#28505d",
                        "bold": true
                    },
                    "disabled-cursor-line": {
                        "color": "$cyan",
                        "background-color": "#18202d"
                    },
                    "time-column": {
                        "background-color": "#112637"
                    },
                    "adjusted-time": {
                        "color": "$magenta"
                    },
                    "skewed-time": {
                        "color": "$yellow"
                    },
                    "offset-time": {
                        "color": "$cyan"
                    },
                    "file-offset": {
                        "color": "#888"
                    },
                    "invalid-msg": {
                        "color": "$yellow"
                    },
                    "focused": {
                        "color": "$black",
                        "background-color": "#666"
                    },
                    "disabled-focused": {
                        "color": "$white",
                        "background-color": "#333"
                    },
                    "popup": {
                        "color": "#fff",
                        "background-color": "#011627"
                    },
                    "popup-border": {
                        "color": "#5f7e97",
                        "background-color": "#011627"
                    },
                    "scrollbar": {
                        "color": "$black",
                        "background-color": "$white"
                    },
                    "h1": {
                        "color": "$magenta",
                        "bold": true
                    },
                    "h2": {
                        "color": "$magenta",
                        "underline": true
                    },
                    "h3": {
                        "color": "$magenta"
                    },
                    "h4": {
                        "underline": true
                    },
                    "h5": {
                        "italic": true
                    },
                    "h6": {
                        "italic": true
                    },
                    "hr": {
                        "color": "#444"
                    },
                    "hyperlink": {
                        "underline": true
                    },
                    "list-glyph": {
                        "color": "$yellow"
                    },
                    "breadcrumb": {
                        "color": "#2d5a80",
                        "bold": true
                    },
                    "table-border": {
                        "color": "$blue"
                    },
                    "table-header": {
                        "bold": true
                    },
                    "quote-border": {
                        "color": "$blue",
                        "background-color": "#15293a"
                    },
                    "quoted-text": {
                        "background-color": "#15293a"
                    },
                    "footnote-border": {
                        "color": "$blue",
                        "background-color": "#15293a"
                    },
                    "footnote-text": {
                        "color": "#eee",
                        "background-color": "#15293a"
                    },
                    "snippet-border": {
                        "color": "$cyan"
                    },
                    "indent-guide": {
                        "color": "#444"
                    }
                },
                "syntax-styles": {
                    "inline-code": {
                        "color": "$red",
                        "background-color": "#15293a"
                    },
                    "quoted-code": {
                        "color": "#eee",
                        "background-color": "#15293a"
                    },
                    "code-border": {
                        "color": "#435362",
                        "background-color": "#15293a"
                    },
                    "object-key": {
                        "color": "semantic()"
                    },
                    "keyword": {
                        "color": "#c792ea"
                    },
                    "string": {
                        "color": "#ecc48d",
                        "bold": true
                    },
                    "comment": {
                        "color": "#5d9899"
                    },
                    "doc-directive": {
                        "color": "#addb67"
                    },
                    "variable": {
                        "color": "#addb67"
                    },
                    "symbol": {
                        "color": "#82aaff"
                    },
                    "number": {
                        "color": "#f78c6c"
                    },
                    "re-special": {
                        "color": "$cyan"
                    },
                    "re-repeat": {
                        "color": "$yellow"
                    },
                    "diff-delete": {
                        "color": "#b03435"
                    },
                    "diff-add": {
                        "color": "#264b33"
                    },
                    "diff-section": {
                        "color": "$magenta"
                    },
                    "spectrogram-low": {
                        "color": "$black",
                        "background-color": "$green",
                        "bold": true
                    },
                    "spectrogram-medium": {
                        "color": "$black",
                        "background-color": "$yellow",
                        "bold": true
                    },
                    "spectrogram-high": {
                        "color": "$black",
                        "background-color": "$red",
                        "bold": true
                    },
                    "file": {
                        "color": "#82aaff"
                    },
                    "null": {
                        "color": "#888"
                    },
                    "ascii-control": {
                        "color": "$green"
                    },
                    "non-ascii": {
                        "color": "$yellow"
                    },
                    "function": {
                        "color": "$cyan"
                    },
                    "separators-references-accessors": {
                        "color": "$red"
                    },
                    "type": {
                        "color": "$blue"
                    }
                },
                "status-styles": {
                    "disabled-title": {
                        "color": "#5394ec",
                        "background-color": "#353535",
                        "bold": true
                    },
                    "title": {
                        "color": "#f8f0f0",
                        "background-color": "#2d5a80",
                        "bold": true
                    },
                    "subtitle": {
                        "color": "#f8f8f0",
                        "background-color": "#005f5f"
                    },
                    "info": {
                        "color": "#aaa",
                        "background-color": "#0F1F2B"
                    },
                    "text": {
                        "color": "#f8f8f0",
                        "background-color": "#162d40"
                    },
                    "warn": {
                        "color": "#b39554",
                        "background-color": "#162d40"
                    },
                    "alert": {
                        "color": "#ef5350",
                        "background-color": "#162d40"
                    },
                    "active": {
                        "color": "#264b33",
                        "background-color": "#162d40"
                    },
                    "inactive": {
                        "color": "#f0f0f0",
                        "background-color": "#0F1F2B"
                    },
                    "inactive-alert": {
                        "color": "#ef5350",
                        "background-color": "#0F1F2B"
                    },
                    "hotkey": {
                        "color": "#2d5a80",
                        "bold": true,
                        "underline": true
                    },
                    "title-hotkey": {
                        "color": "$black",
                        "background-color": "#2d5a80",
                        "bold": true,
                        "underline": true
                    },
                    "suggestion": {
                        "color": "#888"
                    }
                },
                "log-level-styles": {
                    "warning": {
                        "color": "#b39554"
                    },
                    "error": {
                        "color": "#ef5350"
                    },
                    "critical": {
                        "color": "#ef5350"
                    },
                    "fatal": {
                        "color": "#ef5350"
                    }
                }
            }
        }
    }
}